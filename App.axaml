<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="TheMule.App"
             xmlns:local="using:TheMule"
             RequestedThemeVariant="Dark">

  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>
  
  <Application.Styles>
    <FluentTheme />
    <StyleInclude Source="avares://TheMule/Icons.axaml" />
  </Application.Styles>

  <Application.Resources>
    <FontFamily x:Key="BarlowFont">avares://TheMule/Assets/Fonts#Barlow</FontFamily>
  </Application.Resources>
</Application>